use strict;
use warnings;

use UBOS::Host;
use UBOS::Logging;

UBOS::Logging::initialize( 'ubos-admin', 'ubos-ready-initialize', 0, '/etc/ubos/log-system.conf' );
UBOS::Host::setState( 'BootingOrShuttingDown' );

1;
