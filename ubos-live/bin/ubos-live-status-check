use strict;
use warnings;

use UBOS::Live::UbosLive;
use UBOS::Logging;

UBOS::Logging::initialize( 'ubos-live', 'ubos-live-status-check', 0, '/etc/ubos/log-system.conf' );

UBOS::Live::UbosLive::statusHandshake();

1;
